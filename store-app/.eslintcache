0 ? toPrice(0) : toPrice(10);\r\n    Cart.taxPrice = toPrice(.15 * Cart.itemsPrice);\r\n    Cart.totalPrice = Cart.itemsPrice + Cart.shippingPrice + Cart.taxPrice;\r\n\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n        if (success) {\r\n            props.history.push(`/order/${order._id}`)\r\n            dispatch({ type: Types.CREATE_ORDER_RESET });\r\n        }\r\n    }, [success])\r\n\r\n    const placeorderHandler = () => {\r\n        dispatch(\r\n            createOrder({ ...Cart, orderItems: cartItems })\r\n        )\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <CheckOut step1 step2 step3 step4 />\r\n            <div className=\"row top\">\r\n                <div className=\"col-2\">\r\n                    <ul>\r\n                        <li>\r\n                            <div className=\"card card-body\">\r\n                                <h2>Shipping</h2>\r\n                                <p>\r\n                                    <strong>Name:</strong> {name}<br />\r\n                                    <strong>Address:</strong> {address}, {postalCode}, {city}, {country}\r\n                                </p>\r\n                            </div>\r\n                        </li>\r\n                        <li>\r\n                            <div className=\"card card-body\">\r\n                                <h2>Payment</h2>\r\n                                <p>\r\n                                    <strong>Method:</strong> {paymentMethod}\r\n                                </p>\r\n                            </div>\r\n                        </li>\r\n                        <li>\r\n                            <div className=\"card card-body\">\r\n                                <h2>Order Items</h2>\r\n                                <ul>\r\n                                    {\r\n                                        cartItems.map(item => {\r\n                                            return <li key={item.product} >\r\n                                                <div className=\"row\">\r\n                                                    <div>\r\n                                                        <img src={`../../${item.image}`} alt={item.name} className=\"small\" />\r\n                                                    </div>\r\n                                                    <div className=\"min-30\">\r\n                                                        <Link to={`/product/${item.product}`}>{item.name}</Link>\r\n                                                    </div>\r\n                                                    <div>{item.qty}  * {item.price} = $ {item.qty * item.price}</div>\r\n                                                </div>\r\n                                            </li>\r\n                                        })\r\n                                    }\r\n                                </ul >\r\n                            </div>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n                <div className=\"col-1\">\r\n                    <div className=\"card card-body\">\r\n                        <ul>\r\n                            <li>\r\n                                <h2>Order Summary</h2>\r\n                            </li>\r\n                            <li>\r\n                                <div className=\"row\">\r\n                                    <div>Items</div>\r\n                                    <div>${Cart.itemsPrice.toFixed(2)}</div>\r\n                                </div>\r\n                            </li>\r\n                            <li>\r\n                                <div className=\"row\">\r\n                                    <div>Shipping</div>\r\n                                    <div>${Cart.shippingPrice.toFixed(2)}</div>\r\n                                </div>\r\n                            </li>\r\n                            <li>\r\n                                <div className=\"row\">\r\n                                    <div>tax</div>\r\n                                    <div>${Cart.taxPrice.toFixed(2)}</div>\r\n                                </div>\r\n                            </li>\r\n                            <li>\r\n                                <div className=\"row\">\r\n                                    <div><strong>Order Total</strong></div>\r\n                                    <div>\r\n                                        <strong>${Cart.totalPrice.toFixed(2)}</strong>\r\n                                    </div>\r\n                                </div>\r\n                            </li>\r\n                            <li>\r\n                                <button className=\"primary block\" onClick={placeorderHandler} disabled={cartItems.length === 0}>\r\n                                    Place Order</button>\r\n                            </li>\r\n                            {loading && <LoadingBox />}\r\n                            {error && <MessageBox variant=\"danger\">{error}</MessageBox>}\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n","E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Componentes\\Order\\reducer\\orderReducer.js",[],"E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Componentes\\Order\\actions\\orderTypes.js",[],"E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Componentes\\Order\\actions\\orderAction.js",[],"E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Componentes\\Order\\reducer\\orderDtailsReducer.js",[],"E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Pages\\ProductDtails.js",[],"E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Pages\\OrderDetails.js",[],"E:\\Learn\\React\\React Ecommers Project\\store-app\\src\\Componentes\\Order\\reducer\\orderPayRedu